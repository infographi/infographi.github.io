---
import "../../styles/global.css";
import { SITE_TITLE, SITE_DESCRIPTION } from '../../consts';
import BaseHead from '../../components/BaseHead.astro';
import BrandingLayout from "../../layouts/Brandinglayout-fr.astro"
import Footer from "../../components/Footer-fr.astro";
import Header from "../../components/Header-fr.astro";
import SkipContent from "../../components/SkipContent.astro";
---

<!doctype html>
<html lang="fr">
	<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
    <body id="the-body" class:list="grid-background">
        <SkipContent />
        <Header title={SITE_TITLE} />
        <BrandingLayout />
        <Footer />
    </body>
</html>